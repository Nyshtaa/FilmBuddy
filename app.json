{
  "expo": {
    "name": "FilmBuddy",
    "slug": "FilmBuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "filmbuddy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nyshtaaa.FilmBuddy"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.nyshtaaa.FilmBuddy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "307923c1-eca7-45e8-b39a-cf5330230ea1"
      }
    },
    "owner": "nyshtaaa"
  }
}
